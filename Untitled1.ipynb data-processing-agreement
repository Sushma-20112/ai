        "import pdfplumber\n",
        "\n",
        "file_path = \"/content/Data-Processing-Agreement-MetaCompliance-Ltd-23.06.25.pdf\"\n",
        "\n",
        "with pdfplumber.open(file_path) as pdf:\n",
        "    page = pdf.pages[5]  # 6th page (index 0-based)\n",
        "    content = page.extract_text()\n",
        "\n",
        "print(content)\n"
      ],
