        "import pypdfium2\n",
        "pdf = pypdfium2.PdfDocument(file_path)\n",
        "\n",
        "page = pdf.get_page(5)\n",
        "\n",
        "text = page.get_textpage().get_text_bounded()\n",
        "\n",
        "print(text)"
